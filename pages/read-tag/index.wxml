<view class="page">
  <view class="ndef-box" bindtap="onClickNDEFContent">
    <view class="title">标签信息</view>
    <block wx:if="{{ nfcInfo.payload }}">
      <view class="content">
        <view class="label">uuid</view>
        <view>{{ nfcInfo.uuid }}</view>
      </view>
      <view wx:for="{{nfcInfo.data}}" wx:key="*this">
        <view class="content">
          <view class="label">{{ item.key }}</view>
          <view>{{ item.value }}</view>
        </view>
      </view>
    </block>
    <view wx:else class="placeholder">暂无内容</view>
  </view>
</view>